                        "type": "boolean"
                      },
                      "collisionWarningEngaged": {
                        "type": "boolean"
                      },
                      "cruiseControlEngaged": {
                        "type": "boolean"
                      },
                      "emergencyBrakeEngaged": {
                        "type": "boolean"
                      },
